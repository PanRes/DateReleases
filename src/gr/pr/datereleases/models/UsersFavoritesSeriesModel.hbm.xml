<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="gr.pr.datereleases.models.UsersFavoritesSeriesModel" table="users_favorites_series" schema="test_db">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <many-to-one name="usersByUserId" class="gr.pr.datereleases.models.UsersModel">
            <column name="user_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="seriesBySeriesId" class="gr.pr.datereleases.models.SeriesModel">
            <column name="series_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>